{
  "name": "ppwcode-angular-schematics",
  "version": "0.0.0",
  "description": "PPWCode Angular Schematics",
  "scripts": {
    "build": "npm run build:compile && npm run build:copy",
    "build:compile": "tsc -p tsconfig.json",
    "build:copy": "copyfiles -u 1 ./src/**.template ./src/**.json dist && copyfiles package.json dist",
    "test": "npm run build && jasmine src/**/*_spec.js"
  },
  "keywords": [
    "schematics"
  ],
  "author": {
    "name": "Adriaan Peeters",
    "email": "adriaan_peeters@peopleware.be"
  },
  "license": "Apache-2.0",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^9.1.3",
    "@angular-devkit/schematics": "^9.1.3",
    "@schematics/angular": "^9.1.3",
    "@types/lodash": "^4.14.149",
    "lodash": "^4.14.2",
    "typescript": "~3.8.2"
  },
  "devDependencies": {
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.5.0",
    "copyfiles": "^2.2.0",
    "jasmine": "^3.5.0"
  }
}
